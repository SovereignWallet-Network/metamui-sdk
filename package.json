{
    "name": "metamui-sdk",
    "version": "1.0.0",
    "description": "An SDK to implement function calls to the metamui-core",
    "main": "index.js",
    "scripts": {
        "prepare": "yarn build",
        "build": "tsc -b",
        "test": "ts-mocha -p ./tsconfig.json ./ test/**/*.ts ./test/*.ts --timeout 80000 --exit",
        "test:local": "env PROVIDER_NETWORK=local ts-mocha -p ./tsconfig.json ./ test/**/*.ts ./test/*.ts --no-timeouts --exit",
        "generate:docs": "jsdoc2md src/$FILE.ts > docs/$FILE.md",
        "common": "env PROVIDER_NETWORK=local ts-mocha -p ./tsconfig.json ./test/common.ts --no-timeouts --exit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SovereignWallet-Network/metamui-sdk.git"
    },
    "author": "SovereignWallet-Network",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/SovereignWallet-Network/metamui-sdk/issues"
    },
    "homepage": "https://github.com/SovereignWallet-Network/metamui-sdk#readme",
    "devDependencies": {
        "@types/expect": "^24.3.0",
        "@types/mocha": "^9.1.1",
        "@types/sinon": "^10.0.13",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "chai": "^4.3.4",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard-with-typescript": "^22.0.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^15.0.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.0.0",
        "esm": "^3.2.25",
        "jsdoc-to-markdown": "^7.1.0",
        "mocha": "^10.0.0",
        "prettier": "^2.7.1",
        "sinon": "^9.2.2",
        "ts-mocha": "^10.0.0",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.0",
        "typescript": "*"
    },
    "dependencies": {
        "@polkadot/api": "^9.4.1",
        "@polkadot/types": "^9.4.1",
        "@polkadot/util": "^10.1.8",
        "@polkadot/util-crypto": "^10.1.8",
        "axios": "^0.27.2",
        "js-sha256": "^0.9.0",
        "tsc": "^2.0.4",
        "typescript": "*"
    }
}