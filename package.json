{
    "name": "metamui-sdk",
    "version": "1.0.0",
    "description": "An SDk to implement function calls to the metablockchain-core",
    "main": "index.js",
    "scripts": {
        "prepare": "yarn build",
        "build": "babel ./src --presets babel-preset-es2015 --out-dir dist",
        "test": "env PROVIDER_NETWORK=testnet mocha --timeout 80000 --exit",
        "test:local": "env PROVIDER_NETWORK=local mocha --no-timeouts --exit",
        "generate:docs": "jsdoc2md src/$FILE.js > docs/$FILE.md"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fennsaji/metamui-sdk.git"
    },
    "author": "Anubhab Patnaik",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/fennsaji/metamui-sdk/issues"
    },
    "homepage": "https://github.com/fennsaji/metamui-sdk#readme",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard-with-typescript": "^22.0.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^15.0.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.0.0",
        "prettier": "^2.7.1",
        "typescript": "*"
    }
}